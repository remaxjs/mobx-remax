{
  "name": "mobx-remax",
  "version": "6.1.5",
  "description": "Remax bindings for MobX. Create fully reactive components.",
  "source": "src/index.js",
  "main": "dist/mobx-remax.js",
  "jsnext:main": "dist/mobx-remax.module.js",
  "umd:main": "dist/mobx-remax.umd.js",
  "unpkg": "dist/mobx-remax.umd.js",
  "module": "dist/mobx-remax.module.js",
  "react-native": "dist/mobx-remax.rn.module.js",
  "types": "dist/mobx-remax.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/remaxjs/mobx-remax.git"
  },
  "scripts": {
    "prettier": "prettier --write \"**/*.js\" \"**/*.ts\"",
    "lint": "eslint .",
    "test": "jest && yarn test:ts",
    "test:ts": "tsc -p test/ts",
    "test:ci": "jest -i --coverage && yarn test:ts && yarn lint",
    "size": "size-limit",
    "build": "yarn bundle && yarn copytypes && yarn makern",
    "copytypes": "shx cp src/index.d.ts dist/mobx-remax.d.ts",
    "makern": "shx cp dist/mobx-remax.module.js dist/mobx-remax.rn.module.js && yarn replace \"react-dom\" \"react-native\" dist/mobx-remax.rn.module.js --silent",
    "bundle": "microbundle  --jsx React.createElement --external mobx,react,remax,mobx-remax-lite --globals react=React,mobx-react-lite=mobxReactLite --name mobxRemax",
    "watch": "jest --watch"
  },
  "author": "Michel Weststrate",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/remaxjs/mobx/issues"
  },
  "homepage": "https://remaxjs.org",
  "resolutions": {
    "@types/yargs": "12.0.1"
  },
  "peerDependencies": {
    "mobx": "^4.0.0 || ^5.0.0",
    "react": "^16.8.0 || 16.9.0-alpha.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.0",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@testing-library/jest-dom": "^4.0.0",
    "@testing-library/react": "^9.0.2",
    "@types/create-react-class": "^15.6.0",
    "@types/node": "^10.0.0",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.0.13",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.1.0",
    "coveralls": "^3.0.3",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^1.0.0",
    "jest": "^24.0.0",
    "jest-environment-jsdom": "^24.0.0",
    "jest-mock-console": "^1.0.0",
    "lint-staged": "^7.0.5",
    "lodash": "^4.17.4",
    "microbundle": "^0.11.0",
    "mobx": "^5.0.0",
    "prettier": "^1.7.2",
    "prop-types": "^15.6.0",
    "react": "^16.9.0",
    "remax": "^1.3.1",
    "replace": "^1.1.0",
    "request": "^2.83.0",
    "shelljs": "^0.8.3",
    "shx": "^0.3.2",
    "size-limit": "^1.3.2",
    "typescript": "^2.6.0"
  },
  "dependencies": {
    "mobx-react-lite": "^1.4.2"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "mobx",
    "mobservable",
    "react-component",
    "react",
    "reactjs",
    "reactive"
  ],
  "lint-staged": {
    "*.{ts,js}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect",
      "<rootDir>/jest.setup.js"
    ],
    "testURL": "http://127.0.0.1/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
